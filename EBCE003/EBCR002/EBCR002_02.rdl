Function TransApproveStatus(ByVal strStatus As String) As String
Dim strResult As String
Select Case strStatus
Case "1"
strResult = "未傳送"
Case "2"
strResult = "簽核中"
Case "3"
strResult = "已簽核"
Case "4"
strResult = "已抽單"
Case Else
strResult = " "
End Select
Return strResult
End Function
